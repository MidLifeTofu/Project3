    <%- include('includes/header.ejs') -%>

    <h2>Welcome to the Scheduling Website</h2>
        <p>Hello <%= name %></p>
        <p>How is your <%= day %> going?</p>

    <h3>Here is the list of schedules:</h3>
    <% for (let i = 0; i < schedules.length; i++) { %>
        <p><strong>ID:</strong> <%=schedules[i].id%></p>
        <p><strong>User ID:</strong> <%=schedules[i].user_id%></p>
        <p><strong>Day:</strong> <%=schedules[i].day%></p>

    <% } %>

    <%- include('includes/footer.ejs') -%>
  
